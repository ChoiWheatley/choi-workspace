    "clang-diagnostic-error",
    "clang-diagnostic-error",
    "clang-diagnostic-error"
  ],
  "C_Cpp.default.includePath": ["${workspaceFolder}/include"],
  "C_Cpp.intelliSenseEngineFallback": "Enabled"
